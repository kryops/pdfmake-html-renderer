{
  "name": "pdfmake-html-renderer",
  "version": "0.3.1",
  "author": "Michael Manzinger <michael@kryops.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kryops/pdfmake-html-renderer.git"
  },
  "bugs": {
    "url": "https://github.com/kryops/pdfmake-html-renderer/issues"
  },
  "homepage": "https://github.com/kryops/pdfmake-html-renderer#readme",
  "keywords": [
    "pdfmake",
    "pdf"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "svelte": "./dist/svelte/index.js",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "svelte": "./dist/svelte/index.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./server.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs"
    },
    "./dist/index.css": {
      "default": "./dist/index.css"
    }
  },
  "types": "index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "start": "vite --config playground.vite.config.ts",
    "build": "vite build && vite build --ssr && BUILD_GLOBAL=true vite build && mv dist/pdfmake-html-renderer.css dist/index.css && rimraf dist/server/pdfmake-html-renderer.css && svelte-package -i src -o dist/svelte",
    "docs": "vite build --config playground.vite.config.ts",
    "lint:tsc": "tsc --noEmit",
    "lint:svelte": "svelte-check",
    "lint:prettier": "prettier --check --cache '**/*.{js,ts,md}'",
    "lint": "run-p 'lint:*'",
    "prettify": "prettier --write --cache '**/*.{js,ts,md}'",
    "prepublish": "yarn lint && yarn test && publint",
    "test": "yarn build && node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "qrcode": "^1.5.4"
  },
  "peerDependencies": {
    "svelte": "^5.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tsconfig/svelte": "^5.0.4",
    "@types/expect-puppeteer": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/pdfmake": "^0.2.9",
    "@types/qrcode": "^1.5.5",
    "ace-builds": "^1.36.0",
    "css-color-names": "^1.0.1",
    "get-port": "^7.1.0",
    "jest": "^29.7.0",
    "jest-image-snapshot": "^6.4.0",
    "jest-puppeteer": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "pdfmake": "^0.2.12",
    "prettier": "^3.3.3",
    "publint": "^0.2.10",
    "puppeteer": "^21.7.0",
    "rimraf": "^6.0.1",
    "svelte": "^5.15.0",
    "svelte-check": "^4.1.1",
    "svelte-preprocess": "^6.0.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.5"
  },
  "packageManager": "yarn@4.4.1"
}
