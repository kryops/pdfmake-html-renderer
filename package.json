{
  "name": "pdfmake-html-renderer",
  "version": "0.2.0",
  "author": "Michael Strobel <michael@kryops.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kryops/pdfmake-html-renderer.git"
  },
  "bugs": {
    "url": "https://github.com/kryops/pdfmake-html-renderer/issues"
  },
  "homepage": "https://github.com/kryops/pdfmake-html-renderer#readme",
  "keywords": [
    "pdfmake",
    "pdf"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "svelte": "./dist/svelte/index.js",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "svelte": "./dist/svelte/index.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./server.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs"
    },
    "./dist/index.css": {
      "default": "./dist/index.css"
    }
  },
  "types": "index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "start": "vite --config playground.vite.config.ts",
    "build": "vite build && vite build --ssr && BUILD_GLOBAL=true vite build && mv dist/style.css dist/index.css && rimraf dist/server/style.css && svelte-package -i src -o dist/svelte",
    "docs": "vite build --config playground.vite.config.ts",
    "lint:tsc": "tsc --noEmit",
    "lint:svelte": "svelte-check --ignore examples",
    "lint:prettier": "prettier --check --cache '**/*.{js,ts,md}'",
    "lint": "run-p 'lint:*'",
    "prettify": "prettier --write --cache '**/*.{js,ts,md}'",
    "prepublish": "yarn lint && yarn build && jest && publint",
    "test": "yarn build && jest"
  },
  "dependencies": {
    "qrcode": "^1.5.3"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.23.3",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/expect-puppeteer": "^5.0.6",
    "@types/jest": "^29.5.11",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/pdfmake": "^0.2.8",
    "@types/qrcode": "^1.5.5",
    "ace-builds": "^1.32.3",
    "css-color-names": "^1.0.1",
    "get-port": "^7.0.0",
    "jest": "^29.7.0",
    "jest-image-snapshot": "^6.4.0",
    "jest-puppeteer": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "pdfmake": "^0.2.9",
    "prettier": "^3.1.1",
    "publint": "^0.2.7",
    "puppeteer": "^21.7.0",
    "rimraf": "^5.0.5",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  },
  "packageManager": "yarn@4.0.2"
}
